    "husky": "8.0.3",
    "lint-staged": "14.0.1",
    "postcss": "8.4.32",
    "prettier": "3.0.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "18.3.12",
      "@types/react-dom": "18.3.1"
    }
