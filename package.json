    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.14.8",
    "@mui/lab": "5.0.0-alpha.143",
    "@mui/material": "6.1.8",
    "@mui/material-nextjs": "6.1.8",
    "@mui/x-date-pickers": "6.13.0",
    "@vercel/speed-insights": "1.0.10",
    "ahooks": "3.7.10",
    "antd": "5.8.6",
    "axios": "1.6.0",
    "babel-plugin-react-compiler": "19.0.0-beta-0dec889-20241115",
    "cross-env": "7.0.3",
    "dayjs": "1.11.9",
    "lodash": "4.17.21",
    "next": "15.0.3",
    "next-auth": "4.24.6",
    "next-pwa": "5.6.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "9.0.1",
    "socket.io-client": "4.7.4",
    "swr": "2.2.5",
    "wwhooks": "2.0.2",
    "www-components": "1.1.0",
    "zustand": "4.4.1",
    "zustand-middleware-computed": "1.2.1"
  },
  "devDependencies": {
    "@types/lodash": "4.14.198",
